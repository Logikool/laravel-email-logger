    "license": "MIT",
    "authors": [
        {
            "name": "Logikool",
            "email": "opensource@logikool.com"
        }
    ],
    "require": {
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/mail": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "illuminate/events": "~5.5.0|~5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Logikool\\LaravelEmailLogger\\": "src/"
        }
    },
    "extra": {
